<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Doze: does this device support STATE_DOZE?  -->
    <bool name="doze_display_state_supported">true</bool>


   <!-- Udfps vendor code -->
    <integer name="config_udfpsVendorCode">22</integer>

    <!-- Whether to pulse onFingerDown actions instead
        of waking the screen while the screen is off or in lpm. -->
    <bool name="config_pulseOnFingerDown">true</bool>


    <!-- Bottom margin size when fingerprint in display is active -->
    <dimen name="keyguard_indication_margin_bottom">6dp</dimen>

     <!-- Preferred refresh rate at keyguard, if supported by the display. Overrides
          keyguardMaxRefreshRate. -->
     <integer name="config_keyguardRefreshRate">60</integer>

    <!-- Location on the screen of the center of the physical power button. -->
    <dimen name="physical_power_button_center_screen_location_y">1210px</dimen>
</resources>
